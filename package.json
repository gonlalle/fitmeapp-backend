{
	"name": "sakai-vue",
	"version": "3.2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/primefaces/sakai-vue.git"
	},
	"engines": {
		"node": "14.16.0"
	},
	"license": "MIT",
	"scripts": {
		"start": "node index.js",
		"test": "mocha tests/unit/*.js --timeout 15000",
		"ci": "start-server-and-test \"node index.js\" http://localhost:3000/api/v1/users test",
		"test:coverage": "npm run cover:unit && npm run cover:report",
		"cover:unit": "nyc npm run test --exit",
		"cover:report": "nyc --reporter=lcov --reporter=text  --reporter=html mocha \"tests/unit/*.js\" --exit"
	},
	"nyc":{
		"exclude": ["**/passport/*.js", "**/tests/*"]
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"eslint": "^6.7.2",
		"mocha": "^9.2.2",

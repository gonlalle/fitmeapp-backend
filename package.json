		"fs": "0.0.1-security",
		"googleapis": "^39.2.0",
		"http2": "^3.3.7",
		"json": "^11.0.0",
		"moment": "^2.29.1",
		"mongoose": "^6.2.7",
		"nodemon": "^2.0.15",
		"oauth": "^0.9.15",
		"passport": "^0.5.2",
		"passport-local": "^1.0.0",
		"prismjs": "1.9.0",

		"sass-loader": "^8.0.2"
	},
	"dependencies": {
		"@vue/cli-service": "^5.0.1",
		"axios": "0.19.0",
		"bcryptjs": "^2.4.3",
		"chai": "^4.3.6",
		"chai-http": "^4.3.0",
		"chart.js": "3.3.2",
		"connect-mongo": "^4.6.0",
		"cookie-parser": "^1.4.6",
		"core-js": "3.6.5",
		"corps": "^0.1.0",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.17.3",
		"express-basic-auth": "^1.2.1",
		"express-session": "^1.17.2",
		"fetch": "^1.1.0",
		"find-config": "^1.0.0",
